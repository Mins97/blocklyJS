{
  "name": "blockly-react-sample",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "blockly": "^3.20200924.4",
    "firebase-tools": "^9.0.0",
    "react": "^17.0.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "audit": "npm audit fix",
    "start": "react-scripts start",
    "deploy": "npm run build && firebase deploy --only hosting:blocklyjs-50",
    "upload": "git add . && git commit -m \"readme.md 수정중\" && git push",
    "open_firebase": "start chrome https://blocklyjs-50.web.app/",
    "push": "npm run deploy && npm run upload && npm run open_firebase"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./",
  "blocklyDemoConfig": {
    "title": "Blockly in React",
    "description": "Load Blockly in a React project.",
    "files": [
      "build/index.html",
      "build/**/*"
    ]
  },
  "devDependencies": {
    "copy-webpack-plugin": "^7.0.0"
  }
}